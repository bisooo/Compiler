program expressions2;

var x: integer;
    y: integer;

    a: integer;
    b: integer;

    c: integer;
    d: integer;
begin
    x = 3;
    y = 4;

    a = x + y;
    b = y - x;

    writeln(x);
    writeln(y);
    writeln(a);
    writeln(b);

    c = (x + a) * (y - b);

    writeln(c);
end.
