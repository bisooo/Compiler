program nestedBlock;
function foo (x : integer)
begin
    if x >= 10 then
    begin
        for i := 1 to x do
        begin
            writeln(i);
        end
    end
    else
    for i := x downto 1 do
        begin
            writeln(i);
        end
end
begin
    foo(15);
    foo(7);

end.
